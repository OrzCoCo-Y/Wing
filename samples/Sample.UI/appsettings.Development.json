{
  "Consul": {
    "Url": "http://localhost:8500",
    //配置中心Key
    "Key": "dashboard",
    //数据中心
    "DataCenter": "dc1",
    //等待时间,单位：分钟
    "WaitTime": 3
  },
  "RabbitMQ": {
    "HostName": "localhost",
    "UserName": "guest",
    "Password": "guest",
    "VirtualHost": "/",
    "Port": 5672,
    //消息过期时间，单位：毫秒，过期会自动路由到死信队列，小于或等于0则永久有效
    "MessageTTL": 10000,
    "ExchangeName": "product",
    //每次投递消息数量
    "PrefetchCount": 1
  },
  "Redis": {
    "Sentinels": [ "10.10.166.167:36379", "10.10.166.168:36379", "10.10.166.169:36379" ],
    "ConnectionString": "127.0.0.1:6379,password=123456,defaultDatabase=0,prefix=wing_"
  },
  "ConnectionStrings": {
    "Wing": "Data Source=192.168.56.96;User Id=sa;Password=wing123.;Initial Catalog=Wing;TrustServerCertificate=true;Pooling=true;Min Pool Size=1"
  },
  //自动同步实体结构到数据库
  "UseAutoSyncStructure": true,
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*"
}
