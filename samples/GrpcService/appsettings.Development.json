{
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "EndpointDefaults": {
      "Protocols": "Http2"
    }
  },
  "Consul": {
    "Url": "http://localhost:8500",
    "Service": {
      //Http  Grpc
      "Option": "Grpc",
      "HealthCheck": {
        "Url": "localhost:5001",
        //单位：秒
        "Timeout": 10,
        //单位：秒
        "Interval": 10,
        "GRPCUseTLS": false
      },
      "Name": "grpctest",
      "Host": "localhost",
      "Port": 5011,
      "Tag": "test",
      "LoadBalancer": {
        //RoundRobin  WeightRoundRobin LeastConnection
        "Option": "WeightRoundRobin",
        //权重
        "Weight": 4
      },
      "Scheme": "https"
    },
    //定时同步数据时间间隔，单位：秒 小于等于0表示立即响应
    "Interval": 10
  },
  "Jwt": {
    "Secret": "U2FsdGVkX18E82bSYRnqfv7isu6313BQ8QNTs0TcZZK2rwviQw==",
    "Iss": "http://www.aistory.top",
    "Aud": "PingPongBoy",
    //token过期分钟数
    "Expire": 1440
  },
  "ConnectionStrings": {
    "Wing": "Data Source=192.168.56.96;User Id=sa;Password=wing123.;Initial Catalog=Wing;TrustServerCertificate=true;Pooling=true;Min Pool Size=1"
  },
  //自动同步实体结构到数据库
  "UseAutoSyncStructure": true,
  "Apm": {
    // 每隔多少秒进行数据持久化
    "PersistenceSeconds": 3,
    // 只监听的请求Url(包含关系)，多个用英文逗号隔开
    "ToListenerUrl": "",
    // 不监听的请求Url(包含关系)，多个用英文逗号隔开
    "DoNotListenerUrl": "http://localhost:8500,/grpc.health.v1.Health/Check",
    // 只监听的Sql(包含关系)，多个用英文逗号隔开
    "ToListenerSql": "",
    // 不监听的Sql(包含关系)，多个用英文逗号隔开
    "DoNotListenerSql": "APM_HttpTracerDetail,APM_SqlTracerDetail,APM_Tracer,APM_WS_HttpTracer,APM_WS_SqlTracer"
  }
}
