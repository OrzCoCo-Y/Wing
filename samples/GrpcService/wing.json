{
  "Consul": {
    "Url": "http://localhost:8500",
    "Service": {
      //Http  Grpc
      "Option": "Grpc",
      "HealthCheck": {
        "Url": "localhost:5001",
        //单位：秒
        "Timeout": 10,
        //单位：秒
        "Interval": 10,
        "GRPCUseTLS": false
      },
      "Name": "grpctest",
      "Host": "localhost",
      "Port": 5011,
      "Tag": "test",
      "LoadBalancer": {
        //RoundRobin  WeightRoundRobin LeastConnection
        "Option": "WeightRoundRobin",
        //权重
        "Weight": 4
      },
      "Scheme": "https"
    },
    //定时同步数据时间间隔，单位：秒 小于等于0表示立即响应
    "Interval": 10
  },
  "Jwt": {
    "Secret": "U2FsdGVkX18E82bSYRnqfv7isu6313BQ8QNTs0TcZZK2rwviQw==",
    "Iss": "http://www.aistory.top",
    "Aud": "PingPongBoy",
