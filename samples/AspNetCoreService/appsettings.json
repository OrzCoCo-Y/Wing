{
  // 是否启用配置中心，默认启用
  "ConfigCenterEnabled": false,
  "Consul": {
    "Url": "http://localhost:8500",
    "Service": {
      //Http  Grpc
      "Option": "Http",
      "HealthCheck": {
        "Url": "http://localhost:5005/health",
        //单位：秒
        "Timeout": 3,
        //单位：秒
        "Interval": 10
      },
      "Name": "Sample.AspNetCoreService",
      "Host": "localhost",
      "Port": 5005,
      "Tag": "aspnetcore",
      "LoadBalancer": {
        //RoundRobin  WeightRoundRobin LeastConnection
        "Option": "WeightRoundRobin",
        //权重
        "Weight": 40
      },
      //http or https
      "Scheme": "http",
      "Developer": "林桂城"
    },
    //定时同步数据时间间隔，单位：秒 小于等于0表示立即响应
    "Interval": 10,
    //数据中心
    "DataCenter": "dc1",
    //等待时间,单位：分钟
    "WaitTime": 3
  },
  "ConnectionStrings": {
    "Wing": "Data Source=192.168.56.96;User Id=sa;Password=wing123.;Initial Catalog=Wing;TrustServerCertificate=true;Pooling=true;Min Pool Size=1",
    "Sample.Wing": "Data Source=192.168.56.96;User Id=sa;Password=wing123.;Initial Catalog=Sample.Wing;TrustServerCertificate=true;Pooling=true;Min Pool Size=1"
  },
  //自动同步实体结构到数据库
  "UseAutoSyncStructure": true,
  "Policy": {
    // 全局策略
    "Global": {
      //是否启用熔断
      "IsEnableBreaker": true,
      //熔断前出现允许错误几次
      "ExceptionsAllowedBeforeBreaking": 3,
      //熔断多长时间（毫秒）
      "MillisecondsOfBreak": 1000,
      //最多重试几次，如果为0则不重试
      "MaxRetryTimes": 0,
      //重试间隔的毫秒数
      "RetryIntervalMilliseconds": 100,
      //执行超过多少毫秒则认为超时（0表示不检测超时）
      "TimeOutMilliseconds": 100,
      //缓存多少毫秒（0表示不缓存）
      "CacheMilliseconds": 0,
      //缓存Key前缀
      "CacheKeyPrefix": "",
      //缓存Key，默认Key是{CacheKeyPrefix}WingPolicyCache_{命名空间+类名+方法声明+实参}
      "CacheKey": ""

    }
  },
  "RabbitMQ": {
    "HostName": "192.168.56.99",
    "UserName": "admin",
    "Password": "admin",
    "VirtualHost": "/",
    "Port": 5672,
    //消息过期时间，单位：毫秒，过期会自动路由到死信队列，小于或等于0则永久有效
    "MessageTTL": 5000,
    "ExchangeName": "Sample.AspNetCoreService",
    //每次投递消息数量
    "PrefetchCount": 1
  },
  "Apm": {
    // 要监听的请求Url(包含关系)
    "ToListenerUrl": [],
    // 不监听的请求Url(包含关系)
    "DoNotListenerUrl": [],
    // 只监听的Sql(包含关系)
    "ToListenerSql": [],
    // 不监听的Sql(包含关系)
    "DoNotListenerSql": []
  },
  "Jwt": {
    "Secret": "U2FsdGVkX18E82bSYRnqfv7isu6313BQ8QNTs0TcZZK2rwviQw==",
    "Iss": "http://www.aistory.top",
    "Aud": "PingPongBoy",
    //token过期分钟数
    "Expire": 1440
  },
  "Redis": {
    //"Sentinels": [ "10.10.166.167:36379", "10.10.166.168:36379", "10.10.166.169:36379" ],
    "ConnectionString": "192.168.56.99,defaultDatabase=0,prefix=wing_"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*"
}
