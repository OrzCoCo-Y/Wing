{
  "Consul": {
    "Url": "http://localhost:8500",
    "Service": {
      //Http  Grpc
      "Option": "Http",
      "HealthCheck": {
        "Url": "http://localhost:5005/health",
        //单位：秒
        "Timeout": 3,
        //单位：秒
        "Interval": 10
      },
      "Name": "product",
      "Host": "localhost",
      "Port": 5005,
      "Tag": "aspnetcore",
      "LoadBalancer": {
        //RoundRobin  WeightRoundRobin LeastConnection
        "Option": "WeightRoundRobin",
        //权重
        "Weight": 50
      },
      //http or https
      "Scheme": "http",
      "Developer": "林桂城"
    },
    //定时同步数据时间间隔，单位：秒 小于等于0表示立即响应
    "Interval": 10,
    //数据中心
    "DataCenter": "dc1",
    //等待时间,单位：分钟
    "WaitTime": 3
  },
  "RabbitMQ": {
    "HostName": "192.168.56.99",
    "UserName": "admin",
    "Password": "admin",
    "VirtualHost": "/",
    "Port": 5672,
    //消息过期时间，单位：毫秒，过期会自动路由到死信队列，小于或等于0则永久有效
    "MessageTTL": 5000,
    "ExchangeName": "product",
    //每次投递消息数量
    "PrefetchCount": 1
  },
  "Jwt": {
    "Secret": "U2FsdGVkX18E82bSYRnqfv7isu6313BQ8QNTs0TcZZK2rwviQw==",
    "Iss": "http://www.aistory.top",
    "Aud": "PingPongBoy",
    //token过期分钟数
    "Expire": 1440
  },
  "Redis": {
    //"Sentinels": [ "10.10.166.167:36379", "10.10.166.168:36379", "10.10.166.169:36379" ],
    "ConnectionString": "192.168.56.99,defaultDatabase=0,prefix=wing_"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*"
}
