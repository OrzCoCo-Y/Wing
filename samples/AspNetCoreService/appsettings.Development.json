{
  "Consul": {
    "Url": "http://localhost:8500",
    "Service": {
      //Http  Grpc
      "Option": "Http",
      "HealthCheck": {
        "Url": "http://localhost:5005/health",
        //单位：秒
        "Timeout": 10,
        //单位：秒
        "Interval": 30
      },
      "Name": "Sample.AspNetCoreService",
      "Host": "localhost",
      "Port": 5005,
      //"ConfigKey": "product1", //配置中心Key 不填 默认为服务名称
      "Tag": "aspnetcore",
      "LoadBalancer": {
        //RoundRobin  WeightRoundRobin LeastConnection
        "Option": "WeightRoundRobin",
        //权重
        "Weight": 40
      },
      // 配置中心key,默认值为服务名称
      //"ConfigKey": "Sample.AspNetCoreService",
      //http or https
      "Scheme": "http",
      "Developer": "林桂城"
    },
    //定时同步数据时间间隔，单位：秒 小于等于0表示立即响应
    "Interval": 10,
    //数据中心
    "DataCenter": "dc1",
    //等待时间,单位：分钟
    "WaitTime": 3
  },
  "ConnectionStrings": {
    "Wing": "Data Source=192.168.56.96;User Id=sa;Password=wing123.;Initial Catalog=Wing;TrustServerCertificate=true;Pooling=true;Min Pool Size=1",
    "Sample.Wing": "Data Source=192.168.56.96;User Id=sa;Password=wing123.;Initial Catalog=Sample.Wing;TrustServerCertificate=true;Pooling=true;Min Pool Size=1"
  },
  //自动同步实体结构到数据库
  "UseAutoSyncStructure": true,
  "RabbitMQ": {
    "HostName": "192.168.56.99",
    "UserName": "admin",
    "Password": "admin",
    "VirtualHost": "/",
    "Port": 5672,
    //消息过期时间，单位：毫秒，过期会自动路由到死信队列，小于或等于0则永久有效
    "MessageTTL": 5000,
    "ExchangeName": "Sample.AspNetCoreService",
    //每次投递消息数量
    "PrefetchCount": 1
  },
  "Apm": {
    // 每隔多少秒进行数据持久化
    "PersistenceSeconds": 3,
    // 只监听的请求Url(包含关系)，多个用英文逗号隔开
    "ToListenerUrl": "",
    // 不监听的请求Url(包含关系)，多个用英文逗号隔开
    "DoNotListenerUrl": "http://localhost:8500",
    // 只监听的Sql(包含关系)，多个用英文逗号隔开
    "ToListenerSql": "",
    // 不监听的Sql(包含关系)，多个用英文逗号隔开
    "DoNotListenerSql": ""
  },
  "Saga": {
    // 每隔多少秒重试失败事务 默认5分钟
    "RetrySeconds": 300,
    // 每隔多少秒执行数据纠正 默认5分钟
    "CorrectingSeconds": 300
  },
  "Jwt": {
    "Secret": "U2FsdGVkX18E82bSYRnqfv7isu6313BQ8QNTs0TcZZK2rwviQw==",
    "Iss": "http://www.aistory.top",
    "Aud": "PingPongBoy",
    //token过期分钟数
    "Expire": 1440
  },
  "Redis": {
    //"Sentinels": [ "10.10.166.167:36379", "10.10.166.168:36379", "10.10.166.169:36379" ],
    "ConnectionString": "192.168.56.99,defaultDatabase=0,prefix=wing_"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*"
}
