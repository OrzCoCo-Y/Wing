<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <PackageId>Wing</PackageId>
    <Title>Wing</Title>
    <Version>0.8.5</Version>
    <Authors>linguicheng</Authors>
    <Product>Wing</Product>
    <Description>.NET微服务框架</Description>
    <PackageProjectUrl>https://gitee.com/linguicheng/Wing</PackageProjectUrl>
    <Copyright>https://gitee.com/linguicheng/Wing</Copyright>
    <RepositoryUrl>https://gitee.com/linguicheng/Wing</RepositoryUrl>
    <PackageTags>Wing;微服务</PackageTags>
    <IncludeSymbols>False</IncludeSymbols>
    <SymbolPackageFormat>symbols.nupkg</SymbolPackageFormat>
    <PackageOutputPath>..\..\packages</PackageOutputPath>
    <PackageRequireLicenseAcceptance>True</PackageRequireLicenseAcceptance>
    <PackageLicenseFile>LICENSE</PackageLicenseFile>
  </PropertyGroup>

  <Import Project="..\..\build\common.props" />

  <ItemGroup>
    <None Include="..\..\LICENSE">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.AspNetCore.Authentication.JwtBearer" Version="3.0.3" />
    <PackageReference Include="Microsoft.Extensions.Caching.Abstractions" Version="3.1.3" />
    <PackageReference Include="Microsoft.Extensions.Caching.Memory" Version="3.1.3" />
    <PackageReference Include="Microsoft.Extensions.Configuration.Abstractions" Version="3.1.2" />
    <PackageReference Include="Microsoft.Extensions.Configuration.Binder" Version="3.1.2" />
    <PackageReference Include="Microsoft.Extensions.DependencyInjection" Version="3.1.1" />
    <PackageReference Include="Microsoft.Extensions.Hosting.Abstractions" Version="3.1.2" />
    <PackageReference Include="Newtonsoft.Json" Version="12.0.3" />
    <PackageReference Include="Polly" Version="7.2.3" />
  </ItemGroup>

</Project>
